{
  "name": "Whereby Log Exporter",
  "manifest_version": 2,
  "version": "1.1.1",
  "icons": {
    "64": "icons/export.png"
  },
  "browser_action": {
    "default_title": "Whereby Log Exporter",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["jquery.min.js", "content_script.js"],
      "matches": ["https://whereby.com/*"]
    }
  ],
  "permissions": [
    "tabs", "https://whereby.com/*"
  ]
}
